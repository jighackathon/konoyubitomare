<p id="notice"><%= notice %></p>

<h1>Project Members</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>User</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_members.each do |project_member| %>
      <tr>
        <td><%= project_member.project %></td>
        <td><%= project_member.user %></td>
        <td><%= project_member.status %></td>
        <td><%= link_to 'Show', project_member %></td>
        <td><%= link_to 'Edit', edit_project_member_path(project_member) %></td>
        <td><%= link_to 'Destroy', project_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Member', new_project_member_path %>
